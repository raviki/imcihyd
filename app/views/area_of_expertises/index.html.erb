<h1>Listing area_of_expertises</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Filed</th>
      <th>Domain</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @area_of_expertises.each do |area_of_expertise| %>
      <tr>
        <td><%= area_of_expertise.Title %></td>
        <td><%= area_of_expertise.Filed %></td>
        <td><%= area_of_expertise.Domain %></td>
        <td><%= link_to 'Show', area_of_expertise %></td>
        <td><%= link_to 'Edit', edit_area_of_expertise_path(area_of_expertise) %></td>
        <td><%= link_to 'Destroy', area_of_expertise, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Area of expertise', new_area_of_expertise_path %>
